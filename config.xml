<?xml version='1.0' encoding='utf-8'?>
<widget id="sh23.sirherobrine23.bds_maneger" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bds Maneger</name>
    <description>
        An easy way to manage a minecraft server
    </description>
    <author email="support@the-bds-maneger.org" href="https://github.com/Bds-Maneger">
        The Bds Maneger Project
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
